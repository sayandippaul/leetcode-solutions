 public:
     
    void selectionSort(int arr[], int n)
    {
       //code here
       int in=0;
       for(int i=0;i<n;i++){
           
            in=i;
           for(int j=i+1;j<n;j++){
            //   a=min(a,arr[j]);
            if(arr[j]<=arr[in]){
                in=j;
                        }
            // cout<<arr[j]<<" ";
           }
           int t=arr[i];
           arr[i]=arr[in];
           arr[in]=t;
          
        //   cout<<endl;
       }
    }
